NODE_ENV=development
ADMIN_API_PORT=4000
ADMIN_PASETO_LOCAL_KEY=replace-with-32-byte-secret
ADMIN_DATABASE_URL="postgresql://username:password@host:5432/database?schema=public"
ADMIN_RATE_LIMIT_WINDOW_MS=60000
ADMIN_RATE_LIMIT_MAX=60
ADMIN_ACCESS_TOKEN_TTL_SECONDS=300
ADMIN_SESSION_TTL_DAYS=7
