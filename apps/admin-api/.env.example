# ADMIN_COOKIE_DOMAIN=your-domain.example
# ADMIN_COOKIE_SECURE=true
ADMIN_BOOTSTRAP_USERNAME=admin
ADMIN_BOOTSTRAP_PASSWORD=replace-with-secure-password
